{
  "name": "YouTubeDL",
  "version": "0.0.1",
  "slug": "youtubedl",
  "description": "Download videos from YouTube.",
  "url": "https://github.com/samlehman617/hassio-addon-youtube-dl",
  "startup": "application",
  "init": true,
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "map": [
    "config:ro",
    "share:rw",
    "ssl:ro"
  ],
  "options": {
    "log_level": "info",
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "url": "",
    "api": {
      "use_api": false,
      "api_key": "",
      "use_youtube_api": false,
      "youtube_api_key": ""
    }
  },
  "ports": {
    "17442/tcp": 8998
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)",
    "ssl": "bool",
    "cerfile": "str",
    "keyfile": "str",
    "url": "str",
    "api": {
      "use_api": "bool",
      "api_key": "str",
      "use_youtube_api": "bool",
      "youtube_api_key": "str"
    }
  },
  "webui": "https://[HOST]:[PORT:8998]"
}
